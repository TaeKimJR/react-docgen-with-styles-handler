{
  "name": "react-docgen-with-styles-handler",
  "version": "0.0.5",
  "description": "A handler to report withStyles className comments.",
  "repository": {
    "type": "git",
    "url": "https://github.com/TaeKimJR/react-docgen-with-styles-handler"
  },
  "bugs": "https://github.com/TaeKimJR/react-docgen-with-styles-handler/issues",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=8.10.0"
  },
  "main": "dist/main.js",
  "scripts": {
    "build": "rimraf dist/ && babel src/ --out-dir dist/ --ignore **/__tests__,**/__mocks__,**/src/types.js",
    "lint": "eslint . --report-unused-disable-directives",
    "fix": "eslint . --fix --report-unused-disable-directives",
    "prepare": "yarn build",
    "preversion": "yarn lint",
    "start": "cd website && yarn && yarn start",
    "test": "jest --verbose false ./src/handlers/__tests__/withStylesHandler-test.js",
    "test:ci": "yarn lint && yarn flow && yarn test --runInBand",
    "watch": "yarn build --watch"
  },
  "keywords": [
    "react",
    "documentation-generation",
    "withStyles"
  ],
  "author": "Tae Kim",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/runtime": "^7.0.0",
    "ast-types": "^0.12.4",
    "async": "^3.2.2",
    "commander": "^2.19.0",
    "doctrine": "^3.0.0",
    "node-dir": "^0.1.10",
    "strip-indent": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "benchmark": "^2.1.4",
    "cli-table": "^0.3.1",
    "cross-spawn": "^6.0.4",
    "eslint": "^5.7.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "flow-bin": "^0.98.0",
    "jest": "^24.1.0",
    "jest-diff": "^24.0.0",
    "jest-matcher-utils": "^24.0.0",
    "prettier": "^1.14.3",
    "rimraf": "^2.3.2",
    "temp": "^0.9.0"
  },
  "jest": {
    "snapshotSerializers": [
      "./tests/NodePathSerializer.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setupTestFramework.js"
    ],
    "roots": [
      "bin",
      "src"
    ],
    "testRegex": "/__tests__/.*-test\\.js$"
  }
}
